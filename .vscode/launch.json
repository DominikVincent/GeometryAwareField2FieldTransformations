    }
  },
  {
    "name": "ns-train nerfacto depth",
    "type": "python",
    "request": "launch",
    "program": "/YOUR/PATH/HERE/nerfstudio_fork/scripts/train.py",
    "args": [
      "depth-nerfacto",
      "--vis", "wandb",
      "--pipeline.datamanager.camera-optimizer.mode", "off",
      "--save-only-latest-checkpoint", "True",
      "--pipeline.model.predict-normals", "True",
