  },
  {
    "name": "ns-train from config",
    "type": "python",
    "request": "launch",
    "program": "/YOUR/PATH/HERE/nerfstudio_fork/scripts/train.py",
    "args": [
      "nesf",
       "--load_config", "/YOUR/PATH/HERE/nesf_models/tmp/nesf/2023-04-29_175231/auto_semantic_config_small_3_no_pretrain.yml",
