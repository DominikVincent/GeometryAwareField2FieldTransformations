  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome against localhost",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:4000",
      "webRoot": "${workspaceFolder}/nerfstudio/viewer/app"
    },
    {
      "name": "Python: Attach using Process ID",
      "type": "python",
      "request": "attach",
      "processId": "${command:pickProcess}",
      "justMyCode": true
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Python: train",
      "type": "python",
      "request": "launch",
      "program": "scripts/train.py",
      "console": "integratedTerminal",
      "args": [
        "instant_ngp",
        "--viewer.no-enable",
        "--logging.event-writer=wandb"
      ]
    },
    {
      "name": "Python: train ngp",
      "type": "python",
      "request": "launch",
      "program": "scripts/train.py",
      "console": "integratedTerminal",
      "args": ["instant_ngp"]
    },
    {
      "name": "Python: train ngp nerfstudio",
      "type": "python",
      "request": "launch",
      "program": "scripts/train.py",
      "console": "integratedTerminal",
      "args": [
        "instant_ngp",
        // "--experiment_name=nerfstudio_experiment",
        "--logging.local-writer.max-log-size=0",
        "--viewer.no-enable"
        // viewer
        // "--viewer.enable",
        // "--viewer.zmq-url=tcp://127.0.0.1:6001",
        // "--viewer.websocket-port=8014",
        // "--pipeline.model.randomize-background",
        // "--pipeline.model.cone-angle=0.00390625",
        // "nerfstudio-data"
      ]
    },
    {
      "name": "Python: train ngp-friends",
      "type": "python",
      "request": "launch",
      "program": "scripts/train.py",
      "console": "integratedTerminal",
      "args": [
        "instant_ngp",
        "--viewer.enable",
        "--viewer.zmq-url=tcp://127.0.0.1:6001",
        "--viewer.websocket-port=8014",
        "--steps_per_save=1000000",
        "--steps_per_test=1000000",
        "--pipeline.model.density-field-params.base-scale=4",
        "--pipeline.model.density-field-params.num-cascades=1",
        "pipeline.datamanager.train-dataparser:friends-data-parser-config",
        "--pipeline.datamanager.train-dataparser.scene-scale=4"
      ]
    },
    {
      "name": "Python: train semantic_nerfw",
      "type": "python",
      "request": "launch",
      "program": "scripts/train.py",
      "console": "integratedTerminal",
      "args": ["semantic_nerf"]
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false
  },
  {
      "name": "Try NeSF",
      "type": "python",
      "request": "launch",
      "program": "/YOUR/PATH/HERE/nerfstudio_fork/tests/try_nesf.py",
      // "args": "viewer",
      // "args": "wandb",
      "args": "tensorboard",
      "console": "integratedTerminal",
      "env": {
        "CUDA_LAUNCH_BLOCKING": "0",
        "CUDA_VISIBLE_DEVICES": "0",
        // "PYTORCH_CUDA_ALLOC_CONF": "max_split_size_mb=1024"
      },
      "justMyCode": false
  },
  {
    "name": "sweep NeSF",
    "type": "python",
    "request": "launch",
    "program": "/YOUR/PATH/HERE/nerfstudio_fork/scripts/sweep_nesf.py",
    "console": "integratedTerminal",
    "args": "dhollidt/mae-models-project/kfsdevg7",
    "env": {
      // "CUDA_LAUNCH_BLOCKING": "1"
    },
    "justMyCode": false
  },
  {
      "name": "nerfacto",
      "type": "python",
      "request": "launch",
      "program": "/YOUR/PATH/HERE/nerfstudio_fork/scripts/train.py",
      "args": ["nerfacto", "--data", "/data/vision/polina/projects/wmh/dhollidt/datasets/toybox-5/0/", "--vis", "tensorboard"],
      "console": "integratedTerminal",
      "justMyCode": false
  },
  {
      "name": "ns-eval",
      "type": "python",
      "request": "launch",
      "program": "/YOUR/PATH/HERE/nerfstudio_fork/scripts/eval.py",
      "args": [
        "--load-config",
        "/YOUR/PATH/HERE/nesf_models/tmp/nesf/2023-08-03_230622_457035/auto_eval_config.yml",
        "--output-path",
        "/YOUR/PATH/HERE/nesf_models/tmp/nesf/2023-06-29_150157/auto_eval.json",
        "--use-wandb",
        "--name",
        "test_3dmiou",
      ],
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
         // "CUDA_VISIBLE_DEVICES": "1"
     }
  },
  {
    "name": "ns-train from config",
    "type": "python",
    "request": "launch",
    "program": "/YOUR/PATH/HERE/nerfstudio_fork/scripts/train.py",
    "args": ["nesf",
    "--load_config",
    "/YOUR/PATH/HERE/nesf_models/tmp/nesf/2023-07-19_170023_940539/auto_eval_config.yml"
    // "/YOUR/PATH/HERE/toybox-5-depth-normal-models/98/depth-nerfacto/07_04_23/auto_semantic_config.yml"
    ],
    "console": "integratedTerminal",
    "justMyCode": false
  }
  {
    "name": "eval runs",
    "type": "python",
    "request": "launch",
    "program": "/YOUR/PATH/HERE/nerfstudio_fork/train_scripts/eval_sweep.py",
    "args": ["run","surface-multi-cam-eval"],
    "console": "integratedTerminal",
    "justMyCode": false,
    "env": {
       // "CUDA_VISIBLE_DEVICES": "1"
      },
  },
  {
    "name": "ns-train nesf",
    "type": "python",
    "request": "launch",
    "program": "/YOUR/PATH/HERE/nerfstudio_fork/scripts/train.py",
    "args": [
      "nesf",
"--data",
"/data/vision/polina/projects/wmh/dhollidt/datasets/klevr_nesf/0",
